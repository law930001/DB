cflags = -pthread -B /root/anaconda3/envs/db/compiler_compat -Wl,--sysroot=/ -Wsign-compare -DNDEBUG -g -fwrapv -O3 -Wall -Wstrict-prototypes -fPIC -I/root/anaconda3/envs/db/lib/python3.7/site-packages/torch/include -I/root/anaconda3/envs/db/lib/python3.7/site-packages/torch/include/torch/csrc/api/include -I/root/anaconda3/envs/db/lib/python3.7/site-packages/torch/include/TH -I/root/anaconda3/envs/db/lib/python3.7/site-packages/torch/include/THC -I/usr/local/cuda/include -I/root/anaconda3/envs/db/include/python3.7m -c
post_cflags = -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=deform_conv_cuda -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++14
cuda_cflags = -I/root/anaconda3/envs/db/lib/python3.7/site-packages/torch/include -I/root/anaconda3/envs/db/lib/python3.7/site-packages/torch/include/torch/csrc/api/include -I/root/anaconda3/envs/db/lib/python3.7/site-packages/torch/include/TH -I/root/anaconda3/envs/db/lib/python3.7/site-packages/torch/include/THC -I/usr/local/cuda/include -I/root/anaconda3/envs/db/include/python3.7m -c
